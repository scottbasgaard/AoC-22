{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/first": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "no-console": "off"
    }
}
